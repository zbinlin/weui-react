  "devDependencies": {
    "babel-cli": "^6.1.2",
    "babel-core": "^6.1.2",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "babel-preset-stage-0": "^6.1.2",
    "chai": "^3.4.1",
    "css-loader": "^0.22.0",
    "css-modules-require-hook": "^2.0.2",
    "del": "^2.0.2",
    "eslint-plugin-react": "^3.8.0",
    "jest-cli": "^0.7.1",
    "mocha": "^2.3.3",
    "node-sass": "^3.4.1",
    "postcss-modules-extract-imports": "^1.0.0",
